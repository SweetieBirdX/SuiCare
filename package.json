{
  "name": "suicare",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Healthcare Data Management System on Sui Blockchain",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "demo": "npm run build && node dist/demo.js",
    "realistic-demo": "npm run build && node dist/realistic-demo.js",
    "integration-demo": "npm run build && node dist/integration-demo.js",
    "example": "npm run build && node dist/example.js",
    "move:build": "cd health_record_project && sui move build",
    "move:test": "cd health_record_project && sui move test",
    "deploy:testnet": "npm run build && node dist/scripts/deploy-testnet.js",
    "setup:wallet": "bash scripts/setup-wallet.sh",
    "verify:config": "npm run build && node dist/scripts/verify-config.js",
    "test:enoki": "npm run build && node dist/scripts/test-enoki.js"
  },
  "keywords": [
    "healthcare",
    "blockchain",
    "sui",
    "encryption",
    "privacy",
    "identity"
  ],
  "author": "SuiCare Team",
  "license": "MIT",
  "dependencies": {
    "@mysten/enoki": "^0.12.8",
    "@mysten/seal": "^0.9.1",
    "@mysten/sui": "^1.43.1",
    "@mysten/walrus": "^0.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "dotenv": "^16.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
